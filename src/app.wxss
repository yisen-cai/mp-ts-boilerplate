/**app.wxss**/
#container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  padding: 20rpx 0;
  box-sizing: border-box;
  background: #f9f9f9;
  min-height: 100%;
  background: linear-gradient(180deg, rgba(255, 255, 255, 1) 0%, rgba(255, 255, 255, 1) 0%, rgba(255, 234, 234, 1) 100%);
  font-family: serif;
}

page {
  height: 100%;
}

.link {
  color: blue;
}

.pressed {
  transform: scale(1.05);
  box-shadow: 0 15rpx 30rpx 0 whitesmoke;
}

